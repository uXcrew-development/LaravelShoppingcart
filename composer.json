{
    "name": "uxcrew/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Gloudemans",
            "email": "info@robgloudemans.nl"
        }
    ],
    "require": {
        "illuminate/support": "7.0.*|| 8.* ||9.* || 10.*",
        "illuminate/session": "7.0.*|| 8.* ||9.* || 10.*",
        "illuminate/events": "7.0.*|| 8.* ||9.* || 10.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.6",
        "mockery/mockery": "~1.5.1",
        "orchestra/testbench": "~8.2"
    },
    "autoload": {
        "psr-4": {
            "Gloudemans\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gloudemans\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Gloudemans\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Gloudemans\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
